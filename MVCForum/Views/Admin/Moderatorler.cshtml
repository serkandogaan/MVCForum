@using MVCForum.Models
@model List<Uyeler>
@{
    ViewBag.Title = "Moderatorler";
    Layout = "~/Views/Shared/_AdminPageLayout.cshtml";
}

<h2>Moderatörler</h2>

<table>


    <thead>

        <tr>
            <th width="10%">Üye ID</th>
            <th width="20%">Üye Adı</th>
            <th width="20%">Üye Soyadı</th>
            <th width="20%">Üye Maid Adresi</th>
            <th width="20%">Üye Şifresi</th>
            <th width="20%">Üye Rölü</th>
        </tr>

    </thead>

    <tbody>

        @foreach (var moduyeler in Model)
        {
        <tr>

            <td width="10%">@moduyeler.UyeID</td>
            <td width="20%">@moduyeler.UyeAdi</td>
            <td width="20%">@moduyeler.UyeSoyadi</td>
            <td width="20%">@moduyeler.UyeMail</td>
            <td width="20%">@moduyeler.UyeSifre</td>
            <td width="20%">@moduyeler.UyeRol</td>
            <td> <a href="/Admin/UyeYap/@moduyeler.UyeID" class="btn btn-info">Üye Yap</a></td>





        </tr>


        }

    </tbody>


</table>